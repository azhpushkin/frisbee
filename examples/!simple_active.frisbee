active A {
    String name;
    fun A(String name) {
        @name = name;
        println("Spawned new A for " + @name);

    }

    fun void say_hello(String name) {
        println("Hello, " + name);
    }
}

fun void main() {
    A a1 = spawn A("Alice");
    A a2 = spawn A("Bob");
    // a ! say_hello("Stranger");
}


