from student import Student;


fun void main() {
    [Student] ss = [];
    ss.push(Student("Anton", 24, [4, 5, 3, 2]));
    ss.push(Student("Alina", nil, [5, 5, 4, 5]));
    ss.push(Student("Sasha", 23, [3, 3, 3, 3]));

    Int average = 0;
    foreach i in range(ss.size()) {
        // TODO: this should raise an error because ss[i] is of type Student
        average = average + ss[i];
    }
    average = average / ss.size();

    print("Average" + average.to_string());
}
