
fun Int add_squares(Int a, Int b) {
    Int c = a * a;
    return c + (b * b);
}

fun void main() {
    Int b = 2;
    Int a = 3;
    
    Int res = add_squares(a, b);
    println("Res is " +  res.to_string());

    if a == 2 {
        res = res * 2;
    } 
    elif a==1 {
        res = 77;
    }
    else {
        res = res + 1;
    }
    println(res.to_string());
}

