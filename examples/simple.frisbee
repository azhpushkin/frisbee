
fun Int add_squares(Int a, Int b) {
    Int a_s = a * a * 1;
    return a_s + (b * b);
}

fun void main() {
    Int b = 2;
    Int a = 3;
    Int res = add_squares(a, b);
    if a == 2 {
        res = res * 2;
    }
    return res; //  TODO: empty return is return (None)!!
    // TODO: why this "return 9" compiles? must be semantic error!
}
